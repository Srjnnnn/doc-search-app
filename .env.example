# =============================================================================
# EXAMPLE CONFIGURATION - Copy this to .env and fill in your values
# =============================================================================

# Required API Keys
BING_API_KEY=your_bing_search_api_key_here
HUGGINGFACE_TOKEN=hf_your_huggingface_token_here

# Application Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_ENVIRONMENT=development

# GPU Settings (adjust based on your hardware)
CUDA_VISIBLE_DEVICES=0
GPU_MEMORY_UTILIZATION=0.8
TENSOR_PARALLEL_SIZE=1
MAX_MODEL_LEN=4096

# Optional: Deployment Configuration
BEAM_REGISTRY_URL=your-beam-registry.com
BEAM_PROJECT_NAME=doc-search-app